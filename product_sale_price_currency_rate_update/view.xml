<?xml version="1.0" encoding="utf-8"?>

<openerp>
<data>

    <record model="ir.ui.view" id="product.list_price_currency">
        <field name="name">product.list_price_currency</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@string='Procurements']" position="inside">
                <b>
                    <button class="oe_inline oe_stat_button" icon="fa-dollar" string="Update Rates" type="object" name="update_list_prices_currency" />
                </b>
            </xpath>
        </field>
    </record>

</data>
</openerp>